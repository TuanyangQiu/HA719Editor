{
    "manifest_version": 2,
    "name": "HA719 Editor",
    "description": "HA719 Editor",
    "version": "1.0.0",
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "activeTab",
        "tabs",
        "notifications"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/content_script.js"
            ],
            "css": [
                "css/content_style.css"
            ],
            "run_at": "document_end"
        }
    ]
}